#include<iostream>
using namespace std;

void H(int* arr, int l) {
	for (int i = 0; i < l - 1; i++) {
		for (int j = 0; j < l - 1 - i; j++) {
			if (arr[j] > arr[j + 1]) {
				int temp = arr[j];
				arr[j] = arr[j+1];
				arr[j + 1] = temp;

			}

		}
	}
}

void P(int*arr2){
	for (int k = 0; k < 9; k++) {
		cout << arr2[k] << "  ";

	}
}

int main() {
	int arr1[9] = { 8,2,79,54,23,56,5,4,8 };
	int l1 = sizeof(arr1) / sizeof(arr1[1]);
	H(arr1, l1);
	P(arr1);
    cout<<"Everything will be fine...";
	return 0;


}
